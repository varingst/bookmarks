{
  "env": {
    "browser": true,
    "node": true
  },

  "settings": {
    "ecmascript": 5,
  },

  "plugins": [
  ],

  "rules": {
    "strict": 0,
    "quotes": 0,
    "no-unused-vars": 1,
    "camelcase": 1,
    "no-underscore-dangle": 0,
    "semi": [2, "always"]
  },

  "extends": "eslint:recommended"
}
